    dcterms:modified "2021-07-30"^^xsd:date ;
    skos:definition "The lowest (by month and overall) maximum air temperature observed at the site."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Lowest maximum temperature (°C)"@en ;
    skos:topConceptOf cs: ;
.

:Lowest-temperature-C
    a skos:Concept ;
    dcterms:created "2021-07-30"^^xsd:date ;
    dcterms:creator <https://linked.data.gov.au/org/neii> ;
    dcterms:modified "2021-07-30"^^xsd:date ;
    skos:definition "The lowest recorded temperature observed at the site, calculated over all years of record."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Lowest temperature (°C)"@en ;
    skos:topConceptOf cs: ;
