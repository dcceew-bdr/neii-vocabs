:Annual-Average
    a skos:Concept ;
    dcterms:created "2020-08-27"^^xsd:date ;
    dcterms:creator <https://linked.data.gov.au/org/neii> ;
    skos:definition "Average of the annual total streamflow volumes (within each water year) over the stated period of record."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Annual Average"@en ;
    skos:topConceptOf cs: ;
.

:Annual-Baseflow
    a skos:Concept ;
    dcterms:created "2020-08-27"^^xsd:date ;
    dcterms:creator <https://linked.data.gov.au/org/neii> ;
    skos:definition "The component of streamflow supplied by groundwater discharge in a water year."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Annual Baseflow"@en ;
    skos:topConceptOf cs: ;
.

:Annual-Total
    a skos:Concept ;
    dcterms:created "2020-08-27"^^xsd:date ;
    dcterms:creator <https://linked.data.gov.au/org/neii> ;
    skos:definition "Total streamflow volume per water year."@en ;
    skos:inScheme cs: ;
