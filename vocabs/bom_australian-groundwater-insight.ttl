:aquifer
    a skos:Concept ;
    dcterms:created "2017-11-16"^^xsd:date ;
    dcterms:creator <https://linked.data.gov.au/org/neii> ;
    skos:definition "A geological formation or group of formations; able to receive, store and transmit significant quantities of water."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "aquifer"@en ;
    skos:topConceptOf cs: ;
.

:bedrock
    a skos:Concept ;
    dcterms:created "2017-11-16"^^xsd:date ;
    dcterms:creator <https://linked.data.gov.au/org/neii> ;
    skos:definition "A consolidated rock that underlies geological basins. Above the bedrock is usually an area of weathered and unconsolidated rock which can from aquifers."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "bedrock"@en ;
    skos:topConceptOf cs: ;
.

:bore
    a skos:Concept ;
    dcterms:created "2017-11-16"^^xsd:date ;
    dcterms:creator <https://linked.data.gov.au/org/neii> ;
    skos:definition "A hole drilled in the ground, a well or any other excavation used to access groundwater. May be used for observation of groundwater (including water level, pressure or water quality)."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "bore"@en ;
    skos:topConceptOf cs: ;
.

:bore-density
    a skos:Concept ;
    dcterms:created "2017-11-16"^^xsd:date ;
    dcterms:creator <https://linked.data.gov.au/org/neii> ;
    skos:definition "The number of bores per unit area of land."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "bore density"@en ;
    skos:topConceptOf cs: ;
.

:confined-aquifer
    a skos:Concept ;
    dcterms:created "2017-11-16"^^xsd:date ;
    dcterms:creator <https://linked.data.gov.au/org/neii> ;
    skos:definition "An aquifer whose upper surface is confined by an overlaying geological formation. Groundwater in a confined aquifer is typically under pressure."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "confined aquifer"@en ;
    skos:topConceptOf cs: ;
.

:groundwater-entitlement-limit
    a skos:Concept ;
    dcterms:created "2017-11-16"^^xsd:date ;
    dcterms:creator <https://linked.data.gov.au/org/neii> ;
    skos:definition "The theoretical maximum volume of water that may be issued as entitlement within a Groundwater Management Area."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "groundwater entitlement limit"@en ;
    skos:topConceptOf cs: ;
.

:groundwater-level
    a skos:Concept ;
    dcterms:created "2017-11-16"^^xsd:date ;
    dcterms:creator <https://linked.data.gov.au/org/neii> ;
    skos:definition "Groundwater levels, as measured in a bore."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "groundwater level"@en ;
    skos:topConceptOf cs: ;
.

:groundwater-management-area
    a skos:Concept ;
    dcterms:created "2017-11-16"^^xsd:date ;
    dcterms:creator <https://linked.data.gov.au/org/neii> ;
    skos:altLabel "GMA"@en ;
    skos:definition "An area of land that is appointed as an area within which there are rules for managing groundwater."@en ;
    skos:inScheme cs: ;
    skos:notation "GMA" ;
    skos:prefLabel "groundwater management area"@en ;
    skos:topConceptOf cs: ;
.

:hydrogeology
    a skos:Concept ;
    dcterms:created "2017-11-16"^^xsd:date ;
    dcterms:creator <https://linked.data.gov.au/org/neii> ;
    skos:definition "The hydrological and geological science concerned with the occurrence, distribution, quality and movement of groundwater, especially relating to the distribution of aquifers, groundwater flow and groundwater quality."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "hydrogeology"@en ;
    skos:topConceptOf cs: ;
.

:licensed-groundwater-entitlement
    a skos:Concept ;
    dcterms:created "2017-11-16"^^xsd:date ;
    dcterms:creator <https://linked.data.gov.au/org/neii> ;
    skos:definition "A specific volume of water licensed to an individual or business for extraction in a given period (typically annually), according to rules established in the relevant groundwater plan."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "licensed groundwater entitlement"@en ;
    skos:topConceptOf cs: ;
.

:not-available
    a skos:Concept ;
    dcterms:created "2017-11-21"^^xsd:date ;
