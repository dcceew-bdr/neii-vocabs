:Mean-daily-terrestrial-minimum-temperature-C
    a skos:Concept ;
    dcterms:created "2021-07-30"^^xsd:date ;
    dcterms:creator <https://linked.data.gov.au/org/neii> ;
    dcterms:modified "2021-07-30"^^xsd:date ;
    skos:definition "The long-term average daily terrestrial (ground) minimum temperature observed at the site for each month and the year."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Mean daily terrestrial minimum temperature (°C)"@en ;
    skos:topConceptOf cs: ;
.

:Mean-maximum-temperature-C
    a skos:Concept ;
    dcterms:created "2021-07-30"^^xsd:date ;
    dcterms:creator <https://linked.data.gov.au/org/neii> ;
    dcterms:modified "2021-07-30"^^xsd:date ;
    skos:definition "The average daily maximum air temperature, for each month and as an annual statistic, calculated over all years of record."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Mean maximum temperature (°C)"@en ;
    skos:topConceptOf cs: ;
.

