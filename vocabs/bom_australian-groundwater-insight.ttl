    dcterms:modified "2017-11-21"^^xsd:date ;
    skos:altLabel "NA"@en ;
    skos:definition "No data is currently available."@en ;
    skos:inScheme cs: ;
    skos:notation "NA" ;
    skos:prefLabel "not available"@en ;
    skos:topConceptOf cs: ;
.

:salinity
    a skos:Concept ;
    dcterms:created "2017-11-16"^^xsd:date ;
    dcterms:creator <https://linked.data.gov.au/org/neii> ;
    skos:definition "The concentration of soluble salts in a solution, soil or other medium."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "salinity"@en ;
    skos:topConceptOf cs: ;
.

:stock-and-domestic-bores
    a skos:Concept ;
    dcterms:created "2017-11-16"^^xsd:date ;
    dcterms:creator <https://linked.data.gov.au/org/neii> ;
    skos:definition "Bores drilled for extracting groundwater for the purpose of watering stock or to use domestically. Groundwater extracted for these purposes does not require a licence in most regions."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "stock and domestic bores"@en ;
    skos:topConceptOf cs: ;
.

:sustainable-diversion-limit
    a skos:Concept ;
    dcterms:created "2017-11-16"^^xsd:date ;
    dcterms:creator <https://linked.data.gov.au/org/neii> ;
    skos:altLabel "SDL"@en ;
    skos:definition "The Sustainable Diversion Limit specifies how much water can be sustainably taken from a catchment, aquifer or basin as a long term annual extraction volume. Each SDL is defined in the Murray Darling Basin Plan."@en ;
    skos:inScheme cs: ;
    skos:notation "SDL" ;
    skos:prefLabel "sustainable diversion limit"@en ;
    skos:topConceptOf cs: ;
.

:unconfined-aquifer
    a skos:Concept ;
    dcterms:created "2017-11-16"^^xsd:date ;
    dcterms:creator <https://linked.data.gov.au/org/neii> ;
    skos:definition "An aquifer whose upper surface is a water table free to fluctuate in equilibrium with atmospheric pressure."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "unconfined aquifer"@en ;
    skos:topConceptOf cs: ;
.

:watertable
    a skos:Concept ;
    dcterms:created "2017-11-16"^^xsd:date ;
    dcterms:creator <https://linked.data.gov.au/org/neii> ;
    skos:definition "The surface in an unconfined aquifer or confining bed below which the ground is saturated with water."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "watertable"@en ;
    skos:topConceptOf cs: ;
.

