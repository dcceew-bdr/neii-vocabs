:Lowest-terrestrial-temperature-C
    a skos:Concept ;
    dcterms:created "2021-07-30"^^xsd:date ;
    dcterms:creator <https://linked.data.gov.au/org/neii> ;
    dcterms:modified "2021-07-30"^^xsd:date ;
    skos:definition "The lowest terrestrial minimum temperature observed at the site over the period."@en ;
    skos:inScheme cs: ;
    skos:prefLabel "Lowest terrestrial temperature (°C)"@en ;
    skos:topConceptOf cs: ;
